{
  "name": "@trucaption/web",
  "version": "1.6.1",
  "description": "Computer-aided realtime transcription",
  "scripts": {
    "build": "yarn clean && webpack --mode production",
    "test": "yarn clean && webpack --mode development",
    "format": "npx biome format --write .",
    "check": "npx biome check .",
    "crowdin-push": "env-cmd crowdin upload --no-preserve-hierarchy",
    "crowdin-pull": "env-cmd crowdin download",
    "clean": "rimraf ../server/webpack"
  },
